extends layout
block content
  h1= title
  p Welcome to  my world
  button(type='button', class='btn btn-warning', onclick ='doBump()') Press Me

  br
  div#bump Bump the count
  br
  script.
      var count=0;
      var bump=1;
      function doBump()
      {
      count=count+bump;
      bump=bump+1;
       document.getElementById("bump").innerHTML = "Count "+ count +" Bump "+bump;
      }